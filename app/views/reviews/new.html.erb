<h1>New Shelter Review:</h1>

<%= form_tag "/shelters/#{@shelter.id}", method: :post do %>
  <%= label_tag :title %>
  <br>
  <%= text_field_tag :title %>
  <br><br>
  <%= label_tag :rating %>
  <br>
  <%= text_field_tag :rating %>
  <br><br>
  <%= label_tag :content %>
  <br>
  <%= text_field_tag :content %>
  <br><br>
  <%= label_tag :picture %>
  <br>
  <%= text_field_tag :picture %>
  <br><br>
  <%= submit_tag 'Create' %>
<% end %>
